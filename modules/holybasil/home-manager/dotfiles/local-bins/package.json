{
  "name": "holybasil-project-manager",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "b": "run-p bounty:*",
    "bd": "run-p bounty-debug:server bounty:web bounty:admin",
    "bountyt:translator": "cd ~/workspace/OFFICE/conflux-translator && proxychains4 -f ~/Dropbox/sync/proxychains.conf node app.js",
    "bounty-debug:server": "ndb yarn --cwd ~/workspace/OFFICE/bounty-server/ dev",
    "bounty:server": "yarn --cwd ~/workspace/OFFICE/bounty-server/ dev",
    "bounty:web": "yarn --cwd ~/workspace/OFFICE/conflux-bounty start",
    "bounty:admin": "yarn --cwd ~/workspace/OFFICE/bounty-admin start",
    "peacock": "run-p peacock:*",
    "peacock:peacock-dev": "yarn --cwd ~/workspace/OFFICE/peacock start",
    "peacock:sso-dev": "yarn --cwd ~/workspace/OFFICE/SSO start",
    "ssodev": "run-s peacock:sso-dev",
    "ssos": "yarn --cwd ~/workspace/OFFICE/SSO/sso-server dev",
    "ssoslt": "yarn --cwd ~/workspace/OFFICE/SSO/sso-server localtestdev",
    "ssoc": "yarn --cwd ~/workspace/OFFICE/SSO/sso-client dev",
    "bimcss": "yarn --cwd ~/workspace/OFFICE/bimcs-server dev",
    "bimcsc": "yarn --cwd ~/workspace/OFFICE/bimcs-cli dev",
    "bimcsdev": "run-p bimcss bimcsc",
    "kinder": "run-p kindergarten:*",
    "kindergarten:web": "yarn --cwd ~/workspace/OFFICE/kindergarten start",
    "kindergarten:server": "yarn --cwd ~/workspace/OFFICE/kindergarten-server start",
    "lmv": "yarn --cwd ~/workspace/OFFICE/gltflmvviewer dev",
    "lmvns": "yarn --cwd ~/workspace/OFFICE/gltflmvviewer devns",
    "bimv": "yarn --cwd ~/workspace/OFFICE/bimsop-viewer-bimv start",
    "gg": "yarn --cwd ~/workspace/OFFICE/gugong dev",
    "pm2": "run-p ssos ssoc pm2dev",
    "pm2tf": "/usr/local/bin/chunkc tiling::desktop --layout float && cross-env NODE_ENV=test run-p -r ssoslt pm2testf",
    "pm2tb": "cross-env NODE_ENV=test run-p -r pm2dev ssoslt pm2testb",
    "pm2tbw": "cross-env NODE_ENV=test run-p -r pm2dev ssoslt pm2testbw",
    "pm2dev": "yarn --cwd ~/workspace/OFFICE/pm2 dev",
    "pm2testf": "yarn --cwd ~/workspace/OFFICE/pm2 test:frontend",
    "pm2testb": "yarn --cwd ~/workspace/OFFICE/pm2 test:backend -- --no-timeout",
    "pm2testbw": "yarn --cwd ~/workspace/OFFICE/pm2 test:backend:watch  -- --no-timeout",
    "zhongjian": "yarn --cwd ~/workspace/OFFICE/zhongjiandianshang_demo dev",
    "paas": "yarn --cwd ~/workspace/OFFICE/bimtransPAAS/src/ dev",
    "bimsdk:watch": "yarn --cwd ~/workspace/OFFICE/BIMSDK/ watch",
    "bimsdk:serve": "live-server --port=8081 ~/workspace/OFFICE/BIMSDK",
    "bimsdk": "run-p bimsdk:watch bimsdk:serve",
    "upgrade:eamcs-packages": "cd ~/.emacs.d/straight/repos && sh pull-all.sh",
    "upgrade:workspace/OFFICE": "cd ~/workspace/OFFICE/ && sh ~/.emacs.d/straight/repos/pull-all.sh",
    "upgrade:workspace/HOME": "cd ~/workspace/HOME/ && sh ~/.emacs.d/straight/repos/pull-all.sh",
    "upgrade:brew": "brew update && brew upgrade",
    "upgrade:yarn": "yarn global upgrade",
    "upgrade:workspace/THIRD": "cd ~/workspace/THIRD/ && sh ~/.emacs.d/straight/repos/pull-all.sh",
    "up": "run-s upgrade:*"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.2"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "got": "^10.0.4",
    "lodash": "^4.17.15",
    "ramda": "^0.26.1"
  }
}
